                });
                $(".switch_tag").click(function(){
                    if(!$(this).hasClass("switch_tag_now")){
                        var index_now=$(".switch_tag_now").index();
                        var index_click=$(this).index();
                        $(".switch_tag_now").children("img").attr("src","/images/view/tag_"+($(".switch_tag_now").index()+1)+"_1.png");
                        $(".switch_tag_now").removeClass("switch_tag_now");
                        $(this).removeClass("switch_tag_hover");
                        $(this).addClass("switch_tag_now");
                        if(index_click!=4){
                            if(index_click==0){
                                $(".switch_list:eq("+index_click+")").empty();
                                $(".switch_list:eq("+index_click+")").append(lis_1);
                            }
                            if(index_click==3){
                                $(".switch_list:eq("+index_click+")").empty();
                                $(".switch_list:eq("+index_click+")").append(lis_2);
                            }
                            $(".switch_list:eq("+index_now+")").removeClass("active");
                            $(".switch_list:eq("+index_click+")").addClass("active");
                        }
                    }
                });
                $(".switch_left_mask").click(function(){
                    if(!$(".active").is(":animated")){
                        if($(".active").children("li").length>1){
                            $(".active").animate({marginLeft:"-600px"},1000,function(){
                                var li0=$(this).children("li:eq(0)");
                                li0.remove();
                                $(this).append(li0);
                                $(this).animate({marginLeft:"0"},0);
                            });
                        }
                    }
                });
                $(".switch_right_mask").click(function(){
                    if(!$(".active").is(":animated")){
                        if($(".active").children("li").length>1){
                            $(".active").animate({marginLeft:"-600px"},0,function(){
                                var li0=$(this).children("li:eq(-1)");
                                li0.remove();
                                $(this).prepend(li0);
                                $(this).animate({marginLeft:"0"},1000);
                            });
                        }
                    }
                });
                $(".qr-wrap").mouseover(function(){
                    $(".broker-qrimg").show();
                });
                $(".qr-wrap").mouseout(function(){
                    $(".broker-qrimg").hide();
                });
                $(".broker-level").mouseover(function(){
                    $(".tip-box").show();
                });
                $(".broker-level").mouseout(function(){
                    $(".tip-box").hide();
                });
                $(".brokercard-scoredetail").mouseover(function(){
                    $(".brokercard-sd-tip").show();
                });
                $(".brokercard-scoredetail").mouseout(function(){
                    $(".brokercard-sd-tip").hide();
                });
            })();
        </script>
    </body>
</html>