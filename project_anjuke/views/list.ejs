        <script>
            (function(){
                /*定义方法结束*/
                $(".cur_citynew").mouseover(function(){
                    $(".city_panel").show();
                });
                $(".cur_citynew").mouseout(function(){
                    $(".city_panel").hide();
                });
                $("#search-esf").keyup(function(){
                    var value=$(this).val();
                    if(value!==""){
                        $("#search-close").show();
                    }else{
                        $("#search-close").hide();
                    }
                });
                $("#search-close").click(function(){
                    $("#search-esf").val("");
                    $(this).hide();
                });
                /*区域选择事件处理*/
                $(".items>.elems-l>a").click(function(e){
                    if($(this).hasClass("selected-item")){
                        e.preventDefault();
                    }else{
                        $(".items>.elems-l>a.selected-item").removeClass("selected-item");
                        $(this).addClass("selected-item");
                    }
                });
                /*售价选择事件处理*/
                $("#pr_form_apf_id_11>input").mousedown(function(){
                    $("#pricerange_search").show();
                });
                $("#pr_form_apf_id_11>input").blur(function(){
                    $("#pricerange_search").hide();
                });
            })();
        </script>
