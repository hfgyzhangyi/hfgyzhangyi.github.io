                        $(".sub-items").hide();
                        $(".sub-items>a.selected-item").removeClass("selected-item");
                        $(".sub-items:eq(0)>a:eq(0)").addClass("selected-item");
                        $(".sub-items:eq(1)>a:eq(0)").addClass("selected-item");
                        $(".sub-items:eq(2)>a:eq(0)").addClass("selected-item");
                        $(".sub-items:eq(3)>a:eq(0)").addClass("selected-item");
                        $(".items>#domain>a.selected-item").removeClass("selected-item");
                        $(this).addClass("selected-item");
                        if($(this).text()==="浦东"){
                            $("#sub_items_1").show();
                        }else if($(this).text()==="闵行"){
                            $("#sub_items_2").show();
                        }else if($(this).text()==="宝山"){
                            $("#sub_items_3").show();
                        }else if($(this).text()==="徐汇"){
                            $("#sub_items_4").show();
                        }
                    }
                });
                $(".sub-items>a").click(function(e){
                    if($(this).hasClass("selected-item")){
                        e.preventDefault();
                    }else{
                        $(".sub-items>a.selected-item").removeClass("selected-item");
                        $(this).addClass("selected-item");
                    }
                });
                /*售价选择事件处理*/
                $(".items>#price>a").click(function(e){
                    if($(this).hasClass("selected-item")){
                        e.preventDefault();
                    }else{
                        $(".items>#price>a.selected-item").removeClass("selected-item");
                        $(this).addClass("selected-item");
                    }
                });
                $("body").click(function(e){
                    if(e.target.id==="from_price"||e.target.id==="to_price"){
                        $("#pricerange_search").show();
                    }else{
                        $("#pricerange_search").hide();
                    }
                    if(e.target.id==="from_area"||e.target.id==="to_area"){
                        $("#arearange_search").show();
                    }else{
                        $("#arearange_search").hide();
                    }
                });
                /*面积事件处理*/
                $(".items>#area>a").click(function(e){
                    if($(this).hasClass("selected-item")){
                        e.preventDefault();
                    }else{
                        $(".items>#area>a.selected-item").removeClass("selected-item");
                        $(this).addClass("selected-item");
                    }
                });
                /*房型事件处理*/
                $(".items>#layout>a").click(function(e){
                    if($(this).hasClass("selected-item")){
                        e.preventDefault();
                    }else{
                        $(".items>#layout>a.selected-item").removeClass("selected-item");
                        $(this).addClass("selected-item");
                    }
                });
                /*更多下拉框事件处理*/
                $("#condhouseage_id").mouseover(function(){
                    $(this).children("ul").css("visibility","visible");
                });
                $("#condhouseage_id").mouseout(function(){
                    $(this).children("ul").css("visibility","hidden");
                });
                $("#condusetype_id").mouseover(function(){
                    $(this).children("ul").css("visibility","visible");
                });
                $("#condusetype_id").mouseout(function(){
                    $(this).children("ul").css("visibility","hidden");
                });
                $("#condfloor_id").mouseover(function(){
                    $(this).children("ul").css("visibility","visible");
                });
                $("#condfloor_id").mouseout(function(){
                    $(this).children("ul").css("visibility","hidden");
                });
                $("#condarrow_id").mouseover(function(){
                    $(this).children("ul").css("visibility","visible");
                });
                $("#condarrow_id").mouseout(function(){
                    $(this).children("ul").css("visibility","hidden");
                });
                $("#conddecoration_id").mouseover(function(){
                    $(this).children("ul").css("visibility","visible");
                });
                $("#conddecoration_id").mouseout(function(){
                    $(this).children("ul").css("visibility","hidden");
                });
                $("#condeconomics_id").mouseover(function(){
                    $(this).children("ul").css("visibility","visible");
                });
                $("#condeconomics_id").mouseout(function(){
                    $(this).children("ul").css("visibility","hidden");
                });
                /*轮播图事件处理*/
                $(".prev").mouseover(function(){
                    $(this).css("opacity",0.8);
                });
                $(".prev").mouseout(function(){
                    $(this).css("opacity",0.4);
                });
                $(".next").mouseover(function(){
                    $(this).css("opacity",0.8);
                });
                $(".next").mouseout(function(){
                    $(this).css("opacity",0.4);
                });
                $(".luna-58-wrap>ul,.prev,.next").mouseover(function(){
                    $(".prev").show();
                    $(".next").show();
                    clearInterval(carousel_timer);
                });
                $(".luna-58-wrap>ul,.prev,.next").mouseout(function(){
                    $(".prev").hide();
                    $(".next").hide();
                    carousel_timer=setInterval(function(){
                        carousel("-237px",true);
                    },3000);
                });
                $(".prev").click(function(){
                    carousel("-237px",true);
                });
                $(".next").click(function(){
                    carousel("-237px",false);
                });
                $(".parent-2:eq(0)>.tab-2>a").mouseover(function(){
                    $(".current-tab").removeClass("current-tab");
                    $(this).addClass("current-tab");
                    if($(this).text()==="浦东"){
                        $(".parent-2:eq(0)>.parent-3:eq(0)").show();
                    }
                });
                $(".links>li>a").click(function(){
                    if($(this).text()==="房贷计算器"){
                        $("#loan_link").show();
                    }else if($(this).text()==="最新问答"){
                        $("#seo_qa").show();
                    }else if($(this).text()==="网站地图"){
                        $("#map_link").show();
                    }else if($(this).text()==="最新房源"){
                        $("#house_link").show();
                    }else if($(this).text()==="其它城市"){
                        $("#other_city").show();
                    }
                });
                $("body").click(function(e){
                    if($("#loan_link").css("display")==="block"){
                        if(e.target.textContent!=="房贷计算器" && !$(e.target).isChildAndSelfOf("#loan_link")){
                            $("#loan_link").hide();
                        }
                    }
                    if($("#seo_qa").css("display")==="block"){
                        if(e.target.textContent!=="最新问答" && !$(e.target).isChildAndSelfOf("#seo_qa")){
                            $("#seo_qa").hide();
                        }
                    }
                    if($("#map_link").css("display")==="block"){
                        if(e.target.textContent!=="网站地图" && !$(e.target).isChildAndSelfOf("#map_link")){
                            $("#map_link").hide();
                        }
                    }
                    if($("#house_link").css("display")==="block"){
                        if(e.target.textContent!=="最新房源" && !$(e.target).isChildAndSelfOf("#house_link")){
                            $("#house_link").hide();
                        }
                    }
                    if($("#other_city").css("display")==="block"){
                        if(e.target.textContent!=="其它城市" && !$(e.target).isChildAndSelfOf("#other_city")){
                            $("#other_city").hide();
                        }
                    }
                });
                $("#loan_link").mouseleave(function(){
                    $("#loan_link").hide();
                });
                $("#seo_qa").mouseleave(function(){
                    $("#seo_qa").hide();
                });
                $("#map_link").mouseleave(function(){
                    $("#map_link").hide();
                });
                $("#house_link").mouseleave(function(){
                    $("#house_link").hide();
                });
                $("#other_city").mouseleave(function(){
                    $("#other_city").hide();
                });
                $(".list-item").mouseover(function(){
                    $(this).find(".houseListTitle").css({"text-decoration":"underline","color":"#f60"});
                });
                $(".list-item").mouseout(function(){
                    $(this).find(".houseListTitle").css({"text-decoration":"none","color":"#333"});
                });
                $(".list-item").click(function(){
                    $(this).find("a")[0].click();
                });
            })();
        </script>
    </body>
</html>