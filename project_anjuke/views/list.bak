                function carousel(marginLeft,direction){
                    if(direction){
                        var li0=$(".luna-58-wrap>ul>li:eq(0)");
                        if($(".luna-58-wrap>ul").is(":animated")===false){
                            $(".luna-58-wrap>ul").animate({marginLeft:marginLeft},1000,function(){
                                $(".luna-58-wrap>ul").children("li:eq(0)").remove();
                                $(".luna-58-wrap>ul").append(li0);
                                $(".luna-58-wrap>ul").animate({marginLeft:"0px"},0);
                            });
                        }
                    }else{
                        var li0=$(".luna-58-wrap>ul>li:eq(-1)");
                        if($(".luna-58-wrap>ul").is(":animated")===false){
                            $(".luna-58-wrap>ul").animate({marginLeft:marginLeft},0,function(){
                                $(".luna-58-wrap>ul").children("li:eq(-1)").remove();
                                $(".luna-58-wrap>ul").prepend(li0);
                                $(".luna-58-wrap>ul").animate({marginLeft:"0px"},1000);
                            });
                        }
                    }
                }
